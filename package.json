{
  "name": "cdk8s-dashboard",
  "version": "0.1.0",
  "description": "Kubernetes dashboard cdk8s construct library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/eladb/cdk8s-dashboard.git"
  },
  "author": "Elad Ben-Israel <benisrae@amazon.com>",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "jsii",
    "watch": "jsii -w",
    "test": "echo ok",
    "compat": "npx jsii-diff npm:$(node -p \"require('./package.json').name\")",
    "package": "jsii-pacmak",
    "bump": "standard-version",
    "build": "yarn compile && yarn test && yarn run package"
  },
  "devDependencies": {
    "jsii": "^1.5.0",
    "jsii-diff": "^1.5.0",
    "jsii-pacmak": "^1.5.0",
    "jsii-release": "^0.1.5",
    "standard-version": "^8.0.0",
    "cdk8s": "0.20.0",
    "constructs": "2.0.1"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "java": {
        "package": "com.github.eladb.cdk8s.dashboard",
        "maven": {
          "groupId": "com.github.eladb",
          "artifactId": "cdk8s-dashboard"
        }
      },
      "python": {
        "distName": "cdk8s-dashboard",
        "module": "cdk8s_dashboard"
      },
      "dotnet": {
        "namespace": "Eladb.Cdk8s.Dashboard",
        "packageId": "Eladb.Cdk8s.Dashboard"
      }
    }
  },
  "peerDependencies": {
    "cdk8s": "^0.20.0",
    "constructs": "^2.0.1"
  }
}
